<!DOCTYPE html>
<html>
  <head>
    <title>ParkMe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if session[:user_id] %>
    <%= button_to "Log Out", log_out_path, method: :delete  %>
    <% end %>

    <%= yield %>
  </body>
</html>
