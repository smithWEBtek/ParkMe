<h1>Log In</h1>


<%= form_with url: sessions_path, local:true do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password %>
<%= f.password_field :password_digest %>
<%= f.submit 'Log in' %>
<% end %>
